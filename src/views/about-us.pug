	.about-us-contacts
		.container
			form.form.form__actions( method='POST')
				.form__title Contact us now
				.form__subtitle Contact us now and find out all the details about the upcoming photo shoot. Send us a request and we will contact you in 15 minutes.
				.form__inputs
					+input('text', 'name', 'Enter your name')(required)
					+input('email', 'email', 'Enter your email')(required)
					+input('text', 'phone', 'Enter your phone')
					+button('Send request', 'form__submit')(type='submit')
	.price
		img(src='./assets/images/circle/services-2.png' alt='')
		.blue-circle
		.container.price__container
			+tag('price', 'price__tag')
			.price__title Choose your optimal plan
			.price__wrapper
				.price-item
					.price-item__icon
						img(src="https://via.placeholder.com/80x80", alt="")
					.price-item__value 199 <sup>$</sup>
					.price-item__tariff basic
					ul.price-item__list
						li Any event for 1 hours
						li 1 photo in a unique frame
						li Top 10 printed photos
						li Up to 50 photos on CD
						li 1 photographer
					+button('get started', 'price-item__btn')
				.price-item
					.price-item__bg
						img(src='https://via.placeholder.com/360x680' alt='')
					.price-item__icon
						img(src="https://via.placeholder.com/80x80", alt="")
					.price-item__value 399 <sup>$</sup>
					.price-item__tariff ultra
					ul.price-item__list
						li Any event for 3 hours
						li 5 photo in a unique frame
						li Top 20 printed photos
						li Up to 100 photos on CD
						li 2 photographer
					+button('get started', 'price-item__btn')
				.price-item
					.price-item__icon
						img(src="https://via.placeholder.com/80x80", alt="")
					.price-item__value 599 <sup>$</sup>
					.price-item__tariff premium
					ul.price-item__list
						li Any event for 8 hours
						li 10 photo in a unique frame
						li Top 100 printed photos
						li Up to 1000 photos on CD
						li 3-5 photographer
					+button('get started', 'price-item__btn')

