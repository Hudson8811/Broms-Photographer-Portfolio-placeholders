extends layouts/default

block vars
	-
		const page = {
		title: 'About us'
		};




block main
	.top-block
		img(src='./assets/images/circle-big.png' alt='')
		.blue-circle
		.top-block__bg
			img(src='https://via.placeholder.com/1920x450' alt='')
		.container.top-block__container
			+title('About us')
			+breadcrumbs([{href: '#', text: 'Home'}, {href: '#', text: 'About us'}], 'top-block__breadcrumbs')
	.about-team
		.blue-circle.filter
		.about-mission__back-object
			svg
				use(xlink:href='./assets/images/svg/sprite.svg#about-me--camera')
		.about-team__container-wrapper
			.container.about-team__container
				.about-team__text
					+tag('who we are', 'stats__tag')
					.services__title about our team #[span(class="nowrap-span") of professionals] 
					.services__text We are a friendly team of professionals united by one goal to make your dreams and wishes come true so that you are guaranteed to get the expected result. We are always open to cooperation and are ready to support interesting projects.
					.about-team__catalog
						.about-team__catalog-item
							.about-team__catalog-number 283
							.about-team__catalog-text pounds of equipment
						.about-team__catalog-item
							.about-team__catalog-number 859
							.about-team__catalog-text happy clients
						.about-team__catalog-item
							.about-team__catalog-number 942
							.about-team__catalog-text photos for magazines
						.about-team__catalog-item
							.about-team__catalog-number 763
							.about-team__catalog-text studio sessions
				.about-team__frame
					img.about-team__frame-bg(src="https://via.placeholder.com/865x700")
					+play('', 'play-part', true, 'https://www.youtube.com/embed/tEPB7uzKuh4')(data-fancybox)
	
